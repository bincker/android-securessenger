<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    >
    <TextView android:id="@+id_keygen/text_keys_invalid" android:text="@string/keygenactivity_keys_invalid" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
    <Button android:id="@+id_keygen/generate" android:text="@string/keygenactivity_generate" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
    
    <TextView android:id="@+id_keygen/text_generating" android:text="@string/generating" android:visibility="invisible" android:layout_width="wrap_content" android:layout_height="wrap_content"/>
</LinearLayout>
